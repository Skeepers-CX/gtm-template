___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
const makeNumber = require('makeNumber');
const log = require('logToConsole');
const makeTableMap = require("makeTableMap");

// Variables associated to the client's account
const endpoint = data.endpoint || "swa.datap.skeepers.io";
const script = "https://sdk.skeepers.io/" +  data.sid + ".js";
const mapping = {};
if(data.rr_mapping) mapping[data.rr_mapping] = "rr_product_id";

// Initializes the skp function
function getSkp(){
  const skp = copyFromWindow('skp');
  if(skp){
    return skp;
  }
  
  setInWindow('skp', function(){
    var p = [];
    p.push(arguments);
    if(copyFromWindow('skp.p')){
      callInWindow('skp.p.push', p[0]);
    }else{
      setInWindow('skp.p', p);
    }
  });
}

const skp = getSkp();

// Retrieves the sdk script, and add the necessary configs
injectScript(script, data.gtmOnSucess, data.gtmOnFailure, "skp");

callInWindow('skp','config', {
  sid: data.sid,
  endpoint: endpoint,
  mapping : mapping
});

const payload = {};

//Basic properties
for (const k in data) {
  if (data.hasOwnProperty(k)) {
    if (k === 'total' || k === 'discount_amount' || k === 'delivery_cost' || k === 'subtotal' ) {
      payload[k] = makeNumber(data[k]) || payload[k];
    } else if (k === 'currency' || k === 'cart_id' || k === 'variation_id' || k === 'parent_id') {
      payload[k] = data[k] || payload[k];
    }
  }
}

//Product properties
if(data.product_array && data.product_properties){
  const mapping = data.product_properties[0];
  const revMap = {};
 
  if(data.add_properties){
    const addMap = makeTableMap(data.add_properties, "skp_field", "cst_field");
    for(const k in addMap){
      mapping[k] = addMap[k];
    }
  }

  for (const k in mapping) {
    if (mapping.hasOwnProperty(k)) {
      revMap[mapping[k]] = k;
    }
  }

  const remappedProducts = data.product_array.map(p => {
    const items = { product: {} };
    for (const k in revMap) {
      log(k + " " + revMap[k] + " " + p[k]);
      if (p.hasOwnProperty(k) && revMap[k] != "quantity" && revMap[k] != "subtotal" && revMap[k] != 'unit_price' && revMap[k] != 'unit_sale_price') {
        items.product[revMap[k]] = p[k];
      } else if(revMap[k] === "quantity" || revMap[k] === "subtotal") {
        items[revMap[k]] = makeNumber(p[k]);  
      } else if(revMap[k] === 'unit_price' || revMap[k] === 'unit_sale_price'){
        items.product[revMap[k]] = makeNumber(p[k]);
      }
    }
    return items;
  });
 
  if(data.event == 'pageview'){
    payload.product = remappedProducts[0].product;
  }else{
    payload.items = remappedProducts;
  }
}

if(data.event == 'order'){
     payload.order_status = 0;
     payload.order_channel = 2;
}

callInWindow('skp', 'data', {
  events: [{
    name: data.event,
    payload: payload,
    event_id: data.order_id
  }]
});

data.gtmOnSuccess();


